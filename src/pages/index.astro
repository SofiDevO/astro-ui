---
import Title from "../components/atoms/Title.astro";
import Layout from "../layouts/Layout.astro";
import Button from "../components/atoms/Button.astro";
import {
  titleVariants,
  variantClasses,
  colorClasses,
} from "@utils/getColor.js";
import ButtonSelect from "@src/components/molecules/ButtonSelect.astro";
import TitleSelect from "@src/components/molecules/TitleSelect.astro";
import TitleDocs from "@src/components/organisms/TitleDocs.astro";
const docs = await getCollection("docs");
import { getCollection } from "astro:content";

---

<Layout>
  <header>
    <nav>
      <ul>
          {
              docs.map((doc) => (

                  <li><a href={`/docs/${doc.id}`}>{doc.id}</a></a></li>
              ))
          }
      </ul>
  </nav>
  </header>
  <main class="w-full flex flex-col items-start justify-center gap-20">
      <ButtonSelect />


  </main>
</Layout>
